LIB	= $(LIBEVENT)

SOURCES = Entity.cc \
	  EntityList.cc \
	  ListElement.cc \
	  Semaphore.cc \
	  TriggerQueue.cc

OBJECTS = Entity.o \
	  EntityList.o \
	  ListElement.o \
	  Semaphore.o \
	  TriggerQueue.o

SetLocalCppFlags(-I$(ARJUNA_INCLUDES) -I../include -I../../ClassLib/include)

AllTarget($(LIB))

MakeLibrary($(LIB),$(OBJECTS))

MakeObjectCPLUSPLUS(Entity,NOPARAM)
MakeObjectCPLUSPLUS(EntityList,NOPARAM)
MakeObjectCPLUSPLUS(ListElement,NOPARAM)
MakeObjectCPLUSPLUS(Semaphore,NOPARAM)
MakeObjectCPLUSPLUS(TriggerQueue,NOPARAM)
